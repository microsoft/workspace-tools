{
  "name": "@microsoft/workspace-tools-monorepo",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/workspace-tools"
  },
  "workspaces": [
    "packages/*",
    "scripts",
    "!scripts/jest/__fixtures__/*"
  ],
  "scripts": {
    "api": "yarn lage api",
    "build": "yarn lage build api",
    "build:docs": "typedoc",
    "change": "beachball change",
    "checkchange": "beachball check",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lage": "cross-env NODE_OPTIONS=\"--experimental-abortcontroller --no-warnings\" lage",
    "release": "beachball publish -y",
    "release:docs": "yarn build:docs && gh-pages -d docs",
    "test": "yarn lage test"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.34.9",
    "@types/fs-extra": "9.0.13",
    "@types/git-url-parse": "9.0.1",
    "@types/jest": "29.5.1",
    "@types/jju": "1.4.2",
    "@types/js-yaml": "4.0.5",
    "@types/micromatch": "4.0.2",
    "@types/node": "14.18.47",
    "@types/tmp": "0.2.3",
    "@types/yarnpkg__lockfile": "1.1.5",
    "@types/lodash": "4.14.194",
    "beachball": "2.31.12",
    "cross-env": "7.0.3",
    "fs-extra": "10.1.0",
    "gh-pages": "4.0.0",
    "jest": "29.5.0",
    "lage": "2.6.2",
    "prettier": "2.8.8",
    "tmp": "0.2.1",
    "ts-jest": "29.1.0",
    "typedoc": "0.22.15",
    "typescript": "4.5.4"
  },
  "resolutions": {
    "**/beachball/workspace-tools": "npm:workspace-tools@0.34.4",
    "**/lage/workspace-tools": "npm:workspace-tools@0.34.4",
    "**/lage/**/workspace-tools": "npm:workspace-tools@0.34.4"
  }
}
