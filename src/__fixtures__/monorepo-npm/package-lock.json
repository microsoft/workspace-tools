{
  "name": "monorepo-npm",
  "version": "0.1.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "version": "0.1.0",
      "license": "MIT",
      "workspaces": {
        "packages": [
          "packages/*"
        ]
      }
    },
    "node_modules/package-a": {
      "resolved": "packages/package-a",
      "link": true
    },
    "node_modules/package-b": {
      "resolved": "packages/package-b",
      "link": true
    },
    "packages/package-a": {
      "version": "0.1.0",
      "license": "MIT"
    },
    "packages/package-b": {
      "version": "0.1.0",
      "license": "MIT"
    }
  },
  "dependencies": {
    "package-a": {
      "version": "file:packages/package-a"
    },
    "package-b": {
      "version": "file:packages/package-b"
    }
  }
}
